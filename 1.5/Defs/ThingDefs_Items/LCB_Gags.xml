<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>LCB_UsedFleshlight</defName>
    <label>used fleshlight</label>
    <description>A beat up old fleshlight. The rubber is flaking off and the orifice is stretched out and sagging. It smells absolutely foul.</description>
    <graphicData>
      <texPath>Things/Item/Special/LCB_UsedFleshlight</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>25</stackLimit>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>0</MarketValue>
      <MaxHitPoints>15</MaxHitPoints>
      <Mass>0.4</Mass>
      <Flammability>1</Flammability>
	  <DeteriorationRate>1.0</DeteriorationRate>
	  <Beauty>-15</Beauty>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <drawGUIOverlay>true</drawGUIOverlay>
    <tradeability>None</tradeability>
    <techLevel>Industrial</techLevel>
  </ThingDef>

  <ThingDef ParentName="ResourceBase">
    <defName>LCB_UsedDrugPipe</defName>
    <label>used drug pipe</label>
    <description>A used-up drug pipe. Still has someone's grubby fingerprints on it and is cracked with a piece missing. Burnt up residue is caked on the inside.</description>
    <graphicData>
      <texPath>Things/Item/Special/LCB_UsedPipe</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.50</drawSize>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>25</stackLimit>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>0</MarketValue>
      <MaxHitPoints>1</MaxHitPoints>
      <Mass>0.006</Mass>
      <Flammability>0</Flammability>
	  <DeteriorationRate>1.0</DeteriorationRate>
	  <Beauty>-15</Beauty>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <drawGUIOverlay>true</drawGUIOverlay>
    <tradeability>None</tradeability>
    <techLevel>Industrial</techLevel>
  </ThingDef>
  
</Defs>  