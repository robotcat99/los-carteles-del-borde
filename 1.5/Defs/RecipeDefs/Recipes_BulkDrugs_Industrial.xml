<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    
	<RecipeDef>
		<defName>MakeLCB_Fent</defName>
		<label>cook some fent</label>
		<description>Cooks a batch of fent from neutroamine.</description>
		<jobString>Cooking fent.</jobString>
		<workAmount>1000</workAmount>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<workSkill>Intellectual</workSkill>
      		<soundWorking>Recipe_Drug</soundWorking>
		<researchPrerequisite>LCB_SinProduction</researchPrerequisite>
		<requiredGiverWorkType>Research</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>6</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Neutroamine</li>
			</thingDefs>
		   <categories>
				
		   </categories>
		</fixedIngredientFilter>
		<products>
			<LCB_Fent>30</LCB_Fent>
		</products>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<skillRequirements>
			<Intellectual>4</Intellectual>
		</skillRequirements>
	</RecipeDef>

